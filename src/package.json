{
  "name": "oh_scenario_generator",
  "version": "1.0.0",
  "description": "OpenHouse Scenario Generator",
  "main": "index.html",
  "scripts": {
    "run.win": "nw .",
    "run.mac": "/Applications/nwjs-sdk-v0.49.2-osx-x64/nwjs.app/Contents/MacOS/nwjs .",
    "run.mac.release": "/Applications/nwjs.app/Contents/MacOS/nwjs .",
    "build.win": "nwbuild --platforms win32,win64 --buildDir ./../dist/ ./",
    "build.mac": "nwbuild --platforms osx64 --buildDir ./../dist/ ./"
  },
  "window": {
    "title": "OpenHouse Scenario Builder",
    "icon": "./img/icon.png",
    "toolbar": false,
    "width": 1200,
    "height": 800
  },
  "dependencies": {
    "oh_gamedata": "latest"
  }
}
